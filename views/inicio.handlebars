            <div class="form-group">
                <label for="exampleInputPassword1">Número celular</label>
                <input name="number" type="text" class="form-control formInput" id="exampleInputPassword1"
                    placeholder="Número celular" required>
            </div>
            <div class="form-check">
                <input type="checkbox" class="form-check-input formInput" id="exampleCheck1" required>
                <label class="form-check-label" for="exampleCheck1">Acepto política de privacidad</label>
            </div> <br>
            <button type="submit" class="btn btn-light">Enviar</button>
